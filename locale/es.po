msgctxt "field:account.invoice,payment_days:"
msgid "Payment Days"
msgstr "Días de pago"

msgctxt "field:party.party,customer_payment_days:"
msgid "Customer Payment Days"
msgstr "Días de pago de cliente"

msgctxt "field:party.party,supplier_payment_days:"
msgid "Supplier Payment Days"
msgstr "Días de pago de proveedor"

msgctxt "help:party.party,customer_payment_days:"
msgid "Space separated list of payment days. A day must be between 1 and 31."
msgstr ""
"Días de pago separados por un espacio. Un día es un valor entre 1 y 31."

msgctxt "help:party.party,supplier_payment_days:"
msgid "Space separated list of payment days. A day must be between 1 and 31."
msgstr ""
"Días de pago separados por un espacio. Un día es un valor entre 1 y 31."

msgctxt "model:ir.message,text:msg_invalid_customer_payment_days"
msgid "Invalid customer payment days \"%(days)s\" for party \"%(party)s\"."
msgstr "Días de pago del cliente \"%(days)s\" no válidos para la parte \"%(party)s\"."

msgctxt "model:ir.message,text:msg_invalid_supplier_payment_days"
msgid "Invalid supplier payment days \"%(days)s\" for party \"%(party)s\"."
msgstr ""
"Días de pago de proveedores \"%(days)s\" no válidos para la parte "
"\"%(party)s\"."
